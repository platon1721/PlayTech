<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:RouletteApp"
             xmlns:converters1="clr-namespace:Converters;assembly=Converters"
             x:Class="RouletteApp.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme DensityStyle="Normal" />
    </Application.Styles>
    
    <Application.Resources>
        <ResourceDictionary>
            <converters1:MultiplierVisibilityConverter x:Key="MultiplierVisibilityConverter"/>
            
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Dark">
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>